// A program that will generate the multiplication table.
// Problem 8
#include <iostream>

using namespace std;

int main()
{
    int number, i;
    
    cout << "Enter a number: " ;
    cin >> number ;
    cout << "Multiplication table of " << number << "\n" ;
    
    
    for (i = 1; i <= 10; i ++)
    {
        cout << number << " * " << i << " = " << number * i << "\n" ;
    }

    return 0;
}
